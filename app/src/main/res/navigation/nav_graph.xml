<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_editFragment"
            app:destination="@id/editFragment" />
        <action
            android:id="@+id/action_homeFragment_to_basicFragment"
            app:destination="@id/basicFragment" />
        <action
            android:id="@+id/action_homeFragment_to_likedFragment"
            app:destination="@id/likedFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/editFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.EditFragment"
        android:label="fragment_edit"
        tools:layout="@layout/fragment_edit" >
        <action
            android:id="@+id/action_editFragment_to_addWordsFragment"
            app:destination="@id/addWordsFragment" />
        <action
            android:id="@+id/action_editFragment_to_editWordsFragment"
            app:destination="@id/editWordsFragment" />
        <action
            android:id="@+id/action_editFragment_to_addCategoryFragment"
            app:destination="@id/addCategoryFragment" />
        <action
            android:id="@+id/action_editFragment_to_addWordFragment"
            app:destination="@id/addWordFragment" />
    </fragment>
    <fragment
        android:id="@+id/addWordsFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.AddWordsFragment"
        android:label="fragment_add_words"
        tools:layout="@layout/fragment_add_words" />
    <fragment
        android:id="@+id/editWordsFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.EditWordsFragment"
        android:label="fragment_edit_words"
        tools:layout="@layout/fragment_edit_words" />
    <fragment
        android:id="@+id/basicFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.BasicFragment"
        android:label="fragment_basic"
        tools:layout="@layout/fragment_basic" >
        <action
            android:id="@+id/action_basicFragment_to_categoryListFragment"
            app:destination="@id/categoryListFragment" />
    </fragment>
    <fragment
        android:id="@+id/likedFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.LikedFragment"
        android:label="fragment_liked"
        tools:layout="@layout/fragment_liked" />
    <fragment
        android:id="@+id/categoryListFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.CategoryListFragment"
        android:label="fragment_category_list"
        tools:layout="@layout/fragment_category_list" />
    <fragment
        android:id="@+id/addCategoryFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.AddCategoryFragment"
        android:label="fragment_add_category"
        tools:layout="@layout/fragment_add_category" />
    <fragment
        android:id="@+id/addWordFragment"
        android:name="com.uz.dictionaryroom_flowable.dictionaryroomflowable.AddWordFragment"
        android:label="fragment_add_word"
        tools:layout="@layout/fragment_add_word" />
</navigation>